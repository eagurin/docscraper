# OpenAI Configuration
MODEL_NAME=gpt-4                          # OpenAI model to use (gpt-4, gpt-3.5-turbo)
OPENAI_API_KEY=your_api_key_here          # Your OpenAI API key

# Browser Configuration
BROWSER_EXECUTABLE_PATH=/usr/bin/chromium # Path to browser executable
HEADLESS=true                            # Run browser in headless mode
DISABLE_SECURITY=true                    # Disable browser security features for scraping

# Scraping Configuration
MAX_CONCURRENT=3                         # Maximum number of concurrent scraping operations
WAIT_FOR_NETWORK_IDLE=3.0               # Wait time for network idle (seconds)

# Output Configuration
OUTPUT_DIR=docs_output                   # Directory for storing documentation
LOGS_DIR=logs                           # Directory for storing logs